{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "build/**/*",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.json"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    // "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "unplugin-vue-macros/macros-global",
    ]
  }
}
